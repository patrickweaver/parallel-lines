<div id="card">

  <ul id="nav">
    <li>{{{ prevSlide page.url }}}</li>
    <li><a href="../../">Slide List</a></li>
    <li>{{{ nextSlide page.url collections.slides }}}</li>
  </ul>

  
  <h2>{{ this.cardTitle}}</h2>
  <div id="card-content">
    <p>{{{ content }}}</p>
  </div>

  <div id="responsive">
    <button id="hide-card" onclick="hideCard()">Hide Card</button>
    <button id="show-card" onclick="showCard()">Show Card</button>
  </div>
</div>

